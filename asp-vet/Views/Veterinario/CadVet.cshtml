@model asp_vet.Models.ModelVeterinario
@{
    ViewBag.Title = "Cadastro - Veterinário";
}

<h2>Cadastro - Veterinário</h2>
@using (Html.BeginForm("CadVet", "Veterinario", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
